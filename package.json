{
  "name": "gpt-agents",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "NODE_DEBUG=astro astro dev",
    "start": "astro dev",
    "build": "astro check && astro build",
    "preview": "astro preview",
    "astro": "astro",
    "format": "prettier --write \"src/**/*.{js,ts,svelte}\""
  },
  "prettier": {
    "svelteSortOrder": "options",
    "svelteStrictMode": false,
    "svelteBracketNewLine": true,
    "svelteAllowShorthand": true,
    "svelteIndentScriptAndStyle": true
  },
  "dependencies": {
    "@astrojs/check": "^0.7.0",
    "@astrojs/mdx": "^3.1.2",
    "@astrojs/svelte": "^5.6.0",
    "@astrojs/tailwind": "^5.1.0",
    "@fontsource/bebas-neue": "^5.0.20",
    "@tailwindcss/forms": "^0.5.7",
    "astro": "^4.11.5",
    "svelte": "^4.2.18",
    "tailwindcss": "^3.4.4",
    "tom-select": "^2.3.1",
    "typescript": "^5.5.3"
  },
  "devDependencies": {
    "@types/node": "^20.14.10",
    "prettier": "^3.3.3",
    "prettier-plugin-svelte": "^3.2.6"
  }
}
